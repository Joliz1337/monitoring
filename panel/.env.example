# Domain (required for SSL)
DOMAIN=panel.example.com

# Panel Authentication
# These are auto-generated by deploy.sh
PANEL_UID=changeme
PANEL_PASSWORD=changeme

# JWT Settings
JWT_SECRET=jwt-secret-key-change-me
JWT_EXPIRE_MINUTES=1440

# Security
MAX_FAILED_ATTEMPTS=5
BAN_DURATION_SECONDS=900

# PostgreSQL Database
# Password is auto-generated by deploy.sh
POSTGRES_USER=panel
POSTGRES_PASSWORD=panel_secret
POSTGRES_DB=panel

# Ports
PANEL_PORT=443
PANEL_HTTP_PORT=80
